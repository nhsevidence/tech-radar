<div class="row" id="tech-row">
  {% for item in categoryList %}
    <div class="span6 tech-container">
      <h4>{{ item.name }}</h1>
      {% for tech in item.values %}
      <div class="tech {{tech.status}} show" data-status={{tech.status}} data-desc={{tech.url}}>
        <p class="tech-name">{{ tech.name}}</p>
      </div>
        {% endfor %}
    </div>

  {% else %}
    <li>No results</li>
  {% endfor %}
</div>
